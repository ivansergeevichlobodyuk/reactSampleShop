<?xml version="1.0"  encoding="UTF-8"?>
<build>
    <inmportal>
        <public>
            <preremove>off</preremove>
            <state>on</state>
            <path>app</path>
            <path>src</path>
            <path>web</path>
            <path>bin</path>
            <path>tests</path>
            <path>var</path>
            <path>cache.properties</path>
            <path>composer.json</path>
            <path>package.json</path>
            <path>bower.json</path>
            <path>Gruntfile.js</path>
            <path>composer.lock</path>
            <path>karma.conf.js</path>
            <path>.test.jshintrc</path>
            <path>.htaccess</path>
        </public>
        <backupweb>
            <state>off</state>
            <offset>3</offset>
        </backupweb>
        <backupsql>
            <state>off</state>
            <offset>3</offset>
        </backupsql>
        <backuplist>
            <state>on</state>
            <offset>3</offset>
            <path>app/config/parameters.yml</path>
            <path>web/upload</path>
            <path>web/files</path>
            <path>.htaccess</path>
        </backuplist>
        <repository>
            <branch>master</branch>
        </repository>
        <permission>
            <state>on</state>
            <write>var/cache</write>
            <write>var/logs</write>
            <write>var/sessions</write>
            <write>web/uploads</write>
            <execute>bin/wkhtmltopdf</execute>
            <execute>bin/wkhtmltoimage</execute>
        </permission>
    </inmportal>
</build>